      "/dev/"
    ],
    "rootDir": ".",
    "roots": [
      "./test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-flow-map.git"
  },
  "keywords": [
    "JSON",
    "transform"
  ],
  "devDependencies": {
    "jest": "^27.5.1",
    "moleculer": "^0.14.19"
  }
}
