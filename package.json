{
  "name": "imicros-flow-map",
  "version": "0.0.5",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "JSON mapper for usage in imicros-flow",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache --silent",
    "test-json": "jest test/jsonMap.spec.js --no-cache",
    "test-map": "jest test/map.spec.js --no-cache"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "!node_modules/",
      "!/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "rootDir": ".",
    "roots": [
      "./test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-flow-map.git"
  },
  "keywords": [
    "JSON",
    "transform"
  ],
  "devDependencies": {
    "jest": "^28.1.3",
    "moleculer": "^0.14.21"
  },
  "dependencies": {
    "jsonata": "^1.8.6",
    "uuid": "^8.3.2"
  }
}
